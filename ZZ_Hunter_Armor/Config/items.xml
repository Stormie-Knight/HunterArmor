		<display_value name="dHealthMax" value="10,20,30,40,50,60" tier="1,2,3,4,5,6"/>
	</effect_group>

	<property class="SDCS">
		<property name="Prefab" value="Entities/Player/*/Gear/Prefabs/gear*RangerPrefab"/>
		<property name="TransformName" value="body"/>
		<property name="Excludes" value="body"/>
	</property>
</item>

